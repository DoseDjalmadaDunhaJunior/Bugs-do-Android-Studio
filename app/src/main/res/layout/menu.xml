<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="25dp">

    <at.markushi.ui.CircleButton
        android:id="@+id/circleButton3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="89dp"
        android:layout_marginLeft="89dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="43dp"
        android:layout_marginRight="43dp"
        android:layout_marginBottom="46dp"
        android:onClick="adicionar"
        android:src="@drawable/add2"
        app:cb_color="#ffff4444"
        app:cb_pressedRingWidth="8dip"
        app:layout_constraintBottom_toTopOf="@+id/circleButton4"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <at.markushi.ui.CircleButton
        android:id="@+id/circleButton4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:layout_marginBottom="46dp"
        android:src="@drawable/list2"
        app:cb_color="#ffff4444"
        app:cb_pressedRingWidth="8dip"
        app:layout_constraintBottom_toTopOf="@+id/circleButton2"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circleButton3" />

    <at.markushi.ui.CircleButton
        android:id="@+id/circleButton2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        android:layout_marginEnd="53dp"
        android:layout_marginRight="53dp"
        android:layout_marginBottom="46dp"
        android:src="@drawable/config2"
        app:cb_color="#ffff4444"
        app:cb_pressedRingWidth="8dip"
        app:layout_constraintBottom_toTopOf="@+id/circleButton"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circleButton4" />

    <at.markushi.ui.CircleButton
        android:id="@+id/circleButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        android:layout_marginEnd="71dp"
        android:layout_marginRight="71dp"
        android:layout_marginBottom="48dp"
        android:src="@drawable/sair2"
        app:cb_color="#ffff4444"
        app:cb_pressedRingWidth="8dip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circleButton2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginEnd="63dp"
        android:layout_marginRight="63dp"
        android:text="Adicionar senha"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/circleButton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circleButton3"
        app:layout_constraintTop_toTopOf="@+id/circleButton3"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="63dp"
        android:layout_marginRight="63dp"
        android:layout_marginBottom="44dp"
        android:text="Listar senhas"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/circleButton4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circleButton4"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="63dp"
        android:layout_marginRight="63dp"
        android:layout_marginBottom="45dp"
        android:text="Configurações"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/circleButton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circleButton2"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="111dp"
        android:layout_marginRight="111dp"
        android:layout_marginBottom="92dp"
        android:text="Sair"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circleButton"
        tools:ignore="HardcodedText" />

</android.support.constraint.ConstraintLayout>